        {
          "@id": "schema:Text"
        },
        {
          "@id": "schema:CreativeWork"
        }
      ],
      "rdfs:label": {
        "@value": "description",
        "@language": "en"
      },
      "rdfs:comment": {
        "@value": "A description of the item.",
        "@language": "en"
      },
      "@type": "rdf:Property"
    }
  ]
